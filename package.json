{
  "name": "personal-website",
  "license": "UNLICENSED",
  "description": "Personal website",
  "keywords": [
    "Sanity",
    "Next.js",
    "Headless CMS",
    "Visual Editing",
    "Live Content API (LCAPI)",
    "Real-time"
  ],
  "homepage": "https://github.com/sanity-io/sanity-template-nextjs-clean#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-template-nextjs-clean/issues"
  },
  "workspaces": [
    "studio",
    "frontend"
  ],
  "scripts": {
    "typegen:studio": "npm run extract-types --workspace=studio",
    "typegen:frontend": "npm run typegen --workspace=frontend",
    "typegen": "npm-run-all --print-label typegen:*",
    "dev": "npm-run-all --parallel --print-label dev:*",
    "dev:prebuild": "npm run prebuild --workspace=studio",
    "dev:next": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "build:next": "npm run build --workspace=frontend",
    "build:studio": "npm run build --workspace=studio",
    "build": "npm-run-all --parallel --print-label build:*",
    "serve:next": "npm run start --workspace=frontend",
    "deploy:next": "npm run deploy --workspace=frontend",
    "deploy:studio": "npm run deploy --workspace=studio",
    "deploy": "npm-run-all --parallel --print-label deploy:*",
    "format": "prettier --cache --write .",
    "lint": "npm run lint --workspace=frontend",
    "type-check": "npm run type-check --workspaces"
  },
  "devDependencies": {
    "babel-plugin-react-compiler": "^1.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.1"
  }
}
